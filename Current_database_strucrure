        AS $$                                                                                                               DECLARE                                                                                                                   v_group_id UUID;                                                                                                        v_result json;                                                                                                        BEGIN                                                                                                                     -- Insert group                                                                                                         INSERT INTO public.groups (                                                                                               name,                                                                                                                   description,                                                                                                            image_url,                                                                                                              is_private,                                                                                                             created_by                                                                                                            )                                                                                                                       VALUES (                                                                                                                  p_name,                                                                                                                 p_description,                                                                                                          p_image_url,                                                            